package com.basics.Interfaces;


interface addition{
	void add();
	static Integer Sq(Integer x)
	{
		return x+x;
	}
}

interface subtraction{
	void sub();
	static Integer Sq(Integer x)
	{
		return x-x;
	}
}

interface multiply{
	void multi();
	static Integer Sq(Integer x)
	{
		return x*x;
	}
}



